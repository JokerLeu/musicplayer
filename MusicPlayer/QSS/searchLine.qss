QPushButton {
    border-image: url(resource/search.png);  /*搜索按钮图片*/
}

QPushButton:hover {
    border-image: url(resource/search_hover.png);  /*搜索按钮图片*/
}

QLineEdit#SearchLine{
    margin-bottom: 1px;
    border: 4px solid #171719;
    border-radius: 10px;
    color: #555555;
    font: 75 9pt "黑体";
    background: #171719;
}

QLineEdit#SearchLine:pressed {
    color: #D0D0D1;
}